

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
   binaryTargets   = ["native"]
}

datasource db {
  provider = "postgresql"
  // url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  username  String @unique
  password String  
  // posts Post[]
}
