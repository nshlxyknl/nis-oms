

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
}

enum UserRole{
  user 
  admin
}

model User {
  id    Int     @id @default(autoincrement())
  username  String @unique
  password String  
  role UserRole @default(user)  
}
